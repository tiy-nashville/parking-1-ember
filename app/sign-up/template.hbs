<div class="form-container">
  <h2 class="form-container__title">this is the sign up page</h2>

  <form class="location-form"{{action registerUser model on="submit"}}>
    <label>Email</label>
    {{input class="sign-up-input" type="text" placeholder="email" value=model.email}}
    <label>Password</label>
    {{input class="sign-up-input" type="password" placeholder="password" value=model.password}}
    <button class="mvbutton">Submit</button>
  </form>
</div>

{{outlet}}
